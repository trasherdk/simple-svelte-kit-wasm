{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}